        <SequenceItem valuetype="tuple">
            <SequenceItem valuetype="str">{project}.edl</SequenceItem>
            <SequenceItem valuetype="hiero.exporters.FnEDLExportTask.EDLExportPreset">
                <root presetname="hiero.exporters.FnEDLExportTask.EDLExportTask" tasktype="hiero.exporters.FnEDLExportTask.EDLExportTask">
